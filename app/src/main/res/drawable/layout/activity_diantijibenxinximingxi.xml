<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.example.administrator.zhihuidianti.activity.DiantijibenxinximingxiActivity">
    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/darkblue">
        <ImageButton
            android:id="@+id/ib_diantijibenxinximingxi_fanhui"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/btn_fanhui"
            android:background="@color/darkblue"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        电梯基本信息明细"
            android:textColor="@color/white"
            android:textSize="20dp"
            />
    </android.support.v7.widget.Toolbar>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/xinximingxi"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="20dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="设备名称"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_shebeimingcheng"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="36px"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="设备型号"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_shebeixinghao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="36px"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="运行状态"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <TextView
            android:id="@+id/tv_yunxingzhuangtai"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:drawableRight="@mipmap/arrow"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="@dimen/biankuangjvli"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="使用登记编号"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_shiyongdengjibianhao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="36px"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="使用单位设备编号"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_shiyongdanbweishebeibianhao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="36px"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="维护合同编号"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_weihuhetongbianhao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="36px"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="30dp"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <Button
            android:id="@+id/btn_chaxun_diantijibenxinximingxi"
            android:layout_width="100dp"
            android:layout_height="35dp"
            android:textSize="@dimen/main_text_size"
            android:text="查询"
            android:textColor="@color/white"
            android:background="@drawable/button"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <Button
            android:id="@+id/btn_excel_diantijibenxinximingxi"
            android:layout_width="100dp"
            android:layout_height="35dp"
            android:textSize="@dimen/main_text_size"
            android:text="Excel"
            android:textColor="@color/white"
            android:background="@drawable/button"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="20dp"/>
    </LinearLayout>


</LinearLayout>
