            android:orientation="horizontal"
            >
        <TextView
            android:layout_width="@dimen/company_text_margin_left"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="企业网址"
            android:padding="@dimen/list_item_margin_left"
            android:background="@color/light_blue"
            android:textSize="@dimen/company_list_item_text"
            />

            <TextView
                android:id="@+id/tv_enterpriseSite"
                android:paddingLeft="@dimen/company_padding_left"
                android:layout_width="wrap_content"
                android:textColor="@color/black"
                android:layout_height="wrap_content"
                android:textSize="@dimen/company_list_item_text"
                android:layout_gravity="center"/>
        </LinearLayout>
        <View

            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/kuang_grey"></View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="@dimen/company_text_margin_left"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="成立时间"
                android:padding="@dimen/list_item_margin_left"
                android:background="@color/light_blue"
                android:textSize="@dimen/company_list_item_text"
                />

            <TextView
                android:id="@+id/tv_enterpriseEstablishTime"
                android:paddingLeft="@dimen/company_padding_left"
                android:layout_width="wrap_content"
                android:textColor="@color/black"
                android:layout_height="wrap_content"
                android:textSize="@dimen/company_list_item_text"
                android:layout_gravity="center"/>
        </LinearLayout>
</LinearLayout>
</RelativeLayout>
</ScrollView>
