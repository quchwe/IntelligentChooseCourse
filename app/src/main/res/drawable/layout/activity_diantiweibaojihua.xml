<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.example.administrator.zhihuidianti.activity.LGJ.DiantiweibaojihuaActivity">
    <include layout="@layout/titlebar"></include>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="20dp"
        android:src="@mipmap/weibaojihua"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="计划名称"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_jihuamingcheng"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="合同是否超期"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="是"
            android:textSize="@dimen/main_text_size"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="@dimen/biankuangjvli"
            />
        <ImageButton
            android:id="@+id/ib_hetongshifouchaoqi_dtwbjh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:src="@mipmap/anniu"
            android:layout_marginTop="@dimen/activity_horizontal_margin"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="否"
            android:textSize="@dimen/main_text_size"
            android:layout_marginLeft="@dimen/biankuangjvli"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="@dimen/biankuangjvli"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="电梯名称"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_diantimingcheng_dtwbjh"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="维保合同编号"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_weibaohetongbianhao_dtwbjh"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="维保合同截止日期"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <TextView
            android:id="@+id/tv_jiezhiriqi1_dtwbjh"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/spinner_border"
            android:layout_marginTop="@dimen/biankuangjvli"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="至"
            android:layout_marginTop="@dimen/biankuangjvli"/>
        <TextView
            android:id="@+id/tv_jiezhiriqi2_dtwbjh"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/spinner_border"
            android:layout_marginLeft="@dimen/biankuangjvli"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="@dimen/activity_horizontal_margin"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="维护企业名称"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_weihuqiyemingcheng"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="维护人"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <EditText
            android:id="@+id/et_weibaoren"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:text="维保计划类型"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <TextView
            android:id="@+id/tv_weibaojihualeixing"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/main_text_size"
            android:background="@drawable/textview_border"
            android:drawableRight="@mipmap/arrow"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginRight="@dimen/biankuangjvli"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="30dp"
        android:paddingRight="@dimen/activity_horizontal_margin">
        <Button
            android:id="@+id/btn_chaxun_dtwbjh"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:textSize="@dimen/main_text_size"
            android:text="查询"
            android:textColor="@color/white"
            android:background="@drawable/button"
            android:layout_weight="1"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="@dimen/biankuangjvli"/>
        <Button
            android:id="@+id/btn_excel_dtwbjh"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:textSize="@dimen/main_text_size"
            android:text="Excel"
            android:textColor="@color/white"
            android:background="@drawable/button"
            android:layout_weight="1"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="20dp"/>
        <Button
            android:id="@+id/btn_tixing_dtwbjh"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:textSize="@dimen/main_text_size"
            android:text="超期合同提醒"
            android:textColor="@color/white"
            android:background="@drawable/button"
            android:layout_weight="1"
            android:layout_marginTop="@dimen/biankuangjvli"
            android:layout_marginLeft="20dp"/>
    </LinearLayout>


</LinearLayout>
